Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"Interleaver1D", "[", 
    RowBox[{"start_", ",", "end_"}], "]"}], ":=", 
   RowBox[{"(", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"Module", "[", 
      RowBox[{
       RowBox[{"{", 
        RowBox[{
        "order", ",", "w", ",", "c", ",", "sec", ",", "secs", ",", "k"}], 
        "}"}], ",", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"w", "=", 
         RowBox[{"end", "-", "start"}]}], ";", "\[IndentingNewLine]", 
        RowBox[{"If", "[", 
         RowBox[{
          RowBox[{"w", "<", "0"}], ",", 
          RowBox[{"Return", "[", 
           RowBox[{"{", "}"}], "]"}]}], "]"}], ";", "\[IndentingNewLine]", 
        RowBox[{"If", "[", 
         RowBox[{
          RowBox[{"w", "\[Equal]", "0"}], ",", 
          RowBox[{"Return", "[", 
           RowBox[{"{", "start", "}"}], "]"}]}], "]"}], ";", 
        "\[IndentingNewLine]", 
        RowBox[{"order", "=", 
         RowBox[{"{", 
          RowBox[{"start", ",", "end"}], "}"}]}], ";", "\[IndentingNewLine]", 
        RowBox[{"If", "[", 
         RowBox[{
          RowBox[{"w", ">", "1"}], ",", "\[IndentingNewLine]", 
          RowBox[{
           RowBox[{"secs", "=", 
            RowBox[{"{", 
             RowBox[{"{", 
              RowBox[{
               RowBox[{"start", "+", "1"}], ",", 
               RowBox[{"end", "-", "1"}]}], "}"}], "}"}]}], ";", 
           "\[IndentingNewLine]", 
           RowBox[{"While", "[", 
            RowBox[{
             RowBox[{
              RowBox[{"Length", "[", "secs", "]"}], ">", "0"}], ",", 
             "\[IndentingNewLine]", 
             RowBox[{
              RowBox[{"sec", "=", 
               RowBox[{"First", "[", "secs", "]"}]}], ";", 
              RowBox[{"secs", "=", 
               RowBox[{"Drop", "[", 
                RowBox[{"secs", ",", "1"}], "]"}]}], ";", 
              "\[IndentingNewLine]", 
              RowBox[{"w", "=", 
               RowBox[{
                RowBox[{"sec", "[", 
                 RowBox[{"[", "2", "]"}], "]"}], "-", 
                RowBox[{"sec", "[", 
                 RowBox[{"[", "1", "]"}], "]"}]}]}], ";", 
              "\[IndentingNewLine]", 
              RowBox[{"If", "[", 
               RowBox[{
                RowBox[{"w", "\[Equal]", "0"}], ",", 
                RowBox[{
                 RowBox[{"AppendTo", "[", 
                  RowBox[{"order", ",", 
                   RowBox[{"sec", "[", 
                    RowBox[{"[", "1", "]"}], "]"}]}], "]"}], ";", 
                 RowBox[{"Continue", "[", "]"}], ";"}]}], "]"}], ";", 
              "\[IndentingNewLine]", 
              RowBox[{"If", "[", 
               RowBox[{
                RowBox[{"w", "\[Equal]", "1"}], ",", 
                RowBox[{
                 RowBox[{"AppendTo", "[", 
                  RowBox[{"order", ",", 
                   RowBox[{"sec", "[", 
                    RowBox[{"[", "1", "]"}], "]"}]}], "]"}], ";", 
                 RowBox[{"AppendTo", "[", 
                  RowBox[{"secs", ",", 
                   RowBox[{"{", 
                    RowBox[{
                    RowBox[{"sec", "[", 
                    RowBox[{"[", "2", "]"}], "]"}], ",", 
                    RowBox[{"sec", "[", 
                    RowBox[{"[", "2", "]"}], "]"}]}], "}"}]}], "]"}], ";", 
                 RowBox[{"Continue", "[", "]"}], ";"}]}], "]"}], ";", 
              "\[IndentingNewLine]", 
              RowBox[{"c", "=", 
               RowBox[{"Floor", "[", 
                RowBox[{
                 RowBox[{"(", 
                  RowBox[{
                   RowBox[{"sec", "[", 
                    RowBox[{"[", "1", "]"}], "]"}], "+", 
                   RowBox[{"sec", "[", 
                    RowBox[{"[", "2", "]"}], "]"}]}], ")"}], "/", "2"}], 
                "]"}]}], ";", "\[IndentingNewLine]", 
              RowBox[{"AppendTo", "[", 
               RowBox[{"order", ",", "c"}], "]"}], ";", "\[IndentingNewLine]", 
              RowBox[{"AppendTo", "[", 
               RowBox[{"secs", ",", 
                RowBox[{"{", 
                 RowBox[{
                  RowBox[{"sec", "[", 
                   RowBox[{"[", "1", "]"}], "]"}], ",", 
                  RowBox[{"c", "-", "1"}]}], "}"}]}], "]"}], ";", 
              "\[IndentingNewLine]", 
              RowBox[{"AppendTo", "[", 
               RowBox[{"secs", ",", 
                RowBox[{"{", 
                 RowBox[{
                  RowBox[{"c", "+", "1"}], ",", 
                  RowBox[{"sec", "[", 
                   RowBox[{"[", "2", "]"}], "]"}]}], "}"}]}], "]"}], ";"}]}], 
            "\[IndentingNewLine]", "]"}], ";"}]}], "\[IndentingNewLine]", 
         "]"}], ";", "\[IndentingNewLine]", 
        RowBox[{"Return", "[", "order", "]"}], ";"}]}], "\[IndentingNewLine]",
       "]"}], ";"}], "\[IndentingNewLine]", ")"}]}], ";"}]], "Input",
