  3.741642273228232*^9}},
 ExpressionUUID -> "c322ddf5-2243-4c4e-bc84-8f7140023209"],

Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{
    RowBox[{"Interleaver2D", "[", 
     RowBox[{"w_", ",", "h_"}], "]"}], ":=", 
    RowBox[{"(", 
     RowBox[{"Module", "[", 
      RowBox[{
       RowBox[{"{", 
        RowBox[{
        "order", ",", "xs", ",", "ys", ",", "digs", ",", "pos", ",", "levels",
          ",", "n"}], "}"}], ",", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"order", "=", 
         RowBox[{"{", "}"}]}], ";", "\[IndentingNewLine]", 
        RowBox[{"levels", "=", 
         RowBox[{"Max", "[", 
          RowBox[{"Ceiling", "[", 
           RowBox[{"Log2", "[", 
            RowBox[{"{", 
             RowBox[{"w", ",", "h"}], "}"}], "]"}], "]"}], "]"}]}], ";", 
        "\[IndentingNewLine]", 
        RowBox[{"xs", "=", 
         RowBox[{"{", 
          RowBox[{"0", ",", "1", ",", "1", ",", "0"}], "}"}]}], ";", 
        RowBox[{"ys", "=", 
         RowBox[{"{", 
          RowBox[{"0", ",", "1", ",", "0", ",", "1"}], "}"}]}], ";", 
        "\[IndentingNewLine]", 
        RowBox[{"n", "=", "0"}], ";", "\[IndentingNewLine]", 
        RowBox[{"While", "[", 
         RowBox[{
          RowBox[{
           RowBox[{"Length", "[", "order", "]"}], "<", 
           RowBox[{"(", 
            RowBox[{"w", " ", "h"}], ")"}]}], ",", "\[IndentingNewLine]", 
          RowBox[{
           RowBox[{"digs", "=", 
            RowBox[{"PadLeft", "[", 
             RowBox[{
              RowBox[{"IntegerDigits", "[", 
               RowBox[{"n", ",", "4"}], "]"}], ",", "levels", ",", "0"}], 
             "]"}]}], ";", "\[IndentingNewLine]", 
           RowBox[{"pos", "=", 
            RowBox[{"Sum", "[", "\[IndentingNewLine]", 
             RowBox[{
              RowBox[{
               SuperscriptBox["2", 
                RowBox[{"i", "-", "1"}]], 
               RowBox[{"{", 
                RowBox[{
                 RowBox[{"xs", "[", 
                  RowBox[{"[", 
                   RowBox[{
                    RowBox[{"digs", "[", 
                    RowBox[{"[", "i", "]"}], "]"}], "+", "1"}], "]"}], "]"}], 
                 ",", 
                 RowBox[{"ys", "[", 
                  RowBox[{"[", 
                   RowBox[{
                    RowBox[{"digs", "[", 
                    RowBox[{"[", "i", "]"}], "]"}], "+", "1"}], "]"}], 
                  "]"}]}], "}"}]}], ",", "\[IndentingNewLine]", 
              RowBox[{"{", 
               RowBox[{"i", ",", "1", ",", "levels"}], "}"}]}], 
             "\[IndentingNewLine]", "]"}]}], ";", "\[IndentingNewLine]", 
           RowBox[{"If", "[", 
            RowBox[{
             RowBox[{
              RowBox[{
               RowBox[{"pos", "[", 
                RowBox[{"[", "1", "]"}], "]"}], "<", "w"}], "&&", 
              RowBox[{
               RowBox[{"pos", "[", 
                RowBox[{"[", "2", "]"}], "]"}], "<", "h"}]}], ",", 
             RowBox[{"AppendTo", "[", 
              RowBox[{"order", ",", 
               RowBox[{"pos", "+", "1"}]}], "]"}]}], "]"}], ";", 
           "\[IndentingNewLine]", 
           RowBox[{"n", "+=", "1"}], ";"}]}], "\[IndentingNewLine]", "]"}], 
        ";", "\[IndentingNewLine]", "order"}]}], "\[IndentingNewLine]", "]"}],
      ")"}]}], ";"}], "\[IndentingNewLine]"}]], "Input",
 Editable->False,
